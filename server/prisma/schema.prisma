generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DBURL")
}

model dictionary {
  id                  Int         @id @default(autoincrement())
  hash                String      @unique @default(cuid())
  word                String      @db.VarChar(256)
  translation         String      @db.VarChar(256)
  translate_from      Json
  translate_to        Json
  image               String
  pronunciation_from  String
  pronunciation_to    String
  schema_version      String      @db.VarChar(10)
  raw                 String
  created_at          DateTime    @default(now())
  updated_at          DateTime?
}