    "express": "^4.21.1",
    "express-http-proxy": "^2.1.1",
    "firebase-admin": "^12.6.0",
    "google-auth-library": "^9.14.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-http-proxy": "^1.6.6",
    "prisma": "^5.21.1",
